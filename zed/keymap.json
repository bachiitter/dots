[
  {
    "context": "Editor && VimControl && !VimWaiting && !VimObject",
    "bindings": {
      "space g d": "editor::GoToDefinition",
      "space g r": "editor::FindAllReferences",
      "space g shift-i": "editor::GoToImplementation",
      "space shift-d": "editor::GoToTypeDefinition",
      "space g shift-d": "editor::GoToDeclaration",
      "space c a": "editor::ToggleCodeActions",
      "shift-k": "editor::Hover",
      "g c": "editor::ToggleComments",
      "space s f": "file_finder::Toggle",
      "space s g": "workspace::NewSearch",
      "space space": "tab_switcher::Toggle",
      "space i h": "editor::ToggleInlayHints",
      "space q": "diagnostics::Deploy"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-z": "workspace::ToggleZoom",
      "cmd-k": [
        "projects::OpenRecent",
        {
          "create_new_window": false
        }
      ],
      "ctrl-x": "tab_switcher::CloseSelectedItem"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "cmd-t": "workspace::NewTerminal"
    }
  }
]
